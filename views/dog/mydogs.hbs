<main role="main" class="inner cover">
    <div class="text-center pt-3">
        <div class="cover-container d-flex h-100 p-3 mx-auto flex-column">
            <h2>My Dogs</h2>
            <table class="table">
                <thead>
                    <tr>
                        <th scope="col">Name</th>
                        <th scope="col">Sex</th>
                        <th scope="col">Race</th>
                        <th scope="col">Size</th>
                        <th scope="col">Age</th>
                        <th scope="col">Actions</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each foundDogs}}
                    <tr>
                        <td>{{this.name}}</td>
                        <td>{{this.sex}}</td>
                        <td>{{this.race}}</td>
                        <td>{{this.size}}</td>
                        <td>{{this.age}}</td>
                        <td>
                            <form action="/profile-mydog" method="get">
                                <a href="/profile-mydog/{{id}}/edit">Edit</a>
                                <a href="/profile-mydog/{{id}}/delete">Delete</a>
                            </form>
                        </td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</main>
